# LEGO Price RAG API Configuration
# Copy this file to .env and fill in your API keys

# OpenAI API Configuration (Required for AI responses)
OPENAI_API_KEY=your_openai_api_key_here

# Rebrickable API Configuration (Required for LEGO data)
REBRICKABLE_API_KEY=your_rebrickable_api_key_here

# Optional: Custom API URLs
REBRICKABLE_BASE_URL=https://rebrickable.com/api/v3/lego/

# Backend Configuration
PORT=8000

# Frontend Configuration
BACKEND_URL=http://localhost:8000 